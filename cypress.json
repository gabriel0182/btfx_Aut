{
	"testFiles": [
		"**/validateTickers.feature",
		"**/balanceQuickTransfer.feature",
		"**/createLongPosition.feature",
		"**/createShortPosition.feature",
		"**/highestBidOrders.feature"
	],
	"viewportWidth": 1480,
	"viewportHeight": 1020,
	"screenshotOnRunFailure": true,
	"video": false,
	"videoUploadOnPasses": false,
	"defaultCommandTimeout": 50000,
	"numTestsKeptInMemory": 100,
	"responseTimeout": 10000,
	"requestTimeout": 30000,
	"taskTimeout": 20000,
	"execTimeout": 200000,
	"retries": {
		"runMode": 1,
		"openMode": 0
	},
	"pageLoadTimeout": 80000,
	"chromeWebSecurity": false,
	"watchForFileChanges": false,
	"reporter": "mochawesome",
	"reporterOptions": {
		"reportDir": "cypress/report/mochawesome-report",
		"inline": true,
		"overwrite": false,
		"html": false,
		"json": true,
		"timestamp": "mmddyyyy_HHMMss"
	},
	"json.schemas": [
		{
			"fileMatch": ["/cypress.json"],
			"url": "https://raw.githubusercontent.com/cypress-io/cypress/develop/cli/schema/cypress.schema.json"
		}
	]
}
